# Increase volume
"~/.config/Xorg/volume-control.sh increase"
   XF86AudioRaiseVolume

# Decrease volume
"~/.config/Xorg/volume-control.sh decrease"
   XF86AudioLowerVolume

# Decrease volume
"~/.config/Xorg/volume-control.sh toggle-mute"
   XF86AudioMute

"light -S 100"
   XF86MonBrightnessUp

"light -S 1"
   XF86MonBrightnessDown

"~/.config/Xorg/toggle-touchpad.sh"
   XF86TouchpadToggle

"gnome-screenshot -ac"
   Print

"gnome-screenshot -ic"
   Control + Print

