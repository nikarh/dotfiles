"~/.bin/volume-control.sh raise"
   XF86AudioRaiseVolume

"~/.bin/volume-control.sh lower"
   XF86AudioLowerVolume

"~/.bin/volume-control.sh mute"
   XF86AudioMute

"~/.bin/backlight-control.sh raise"
   XF86MonBrightnessUp

"~/.bin/backlight-control.sh lower"
   XF86MonBrightnessDown

"~/.bin/toggle-touchpad.sh"
   XF86TouchpadToggle

"flameshot gui"
   Print
   
"flameshot screen"
   Control + Print

"~/.bin/toggle-coolerboost.sh"
   Scroll_Lock

