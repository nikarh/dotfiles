# Increase volume
"~/.control.sh volume inc"
   XF86AudioRaiseVolume

# Decrease volume
"~/.control.sh volume dec"
   XF86AudioLowerVolume

# Decrease volume
"~/.control.sh volume toggle"
   XF86AudioMute

"xbacklight -inc 8"
   XF86MonBrightnessUp

"xbacklight -dec 8"
   XF86MonBrightnessDown

"~/.control.sh touchpad toggle"
   XF86TouchpadToggle
