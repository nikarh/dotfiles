# Increase volume
"~/.config/Xorg/control/bin/control volume inc"
   XF86AudioRaiseVolume

# Decrease volume
"~/.config/Xorg/control/bin/control volume dec"
   XF86AudioLowerVolume

# Decrease volume
"~/.config/Xorg/control/bin/control volume toggle"
   XF86AudioMute

#"~/.config/Xorg/control/bin/control brightness inc"
"light -A 8"
   XF86MonBrightnessUp

#"~/.config/Xorg/control/bin/control brightness dec"
"light -U 8"
   XF86MonBrightnessDown

"~/.config/Xorg/control.sh touchpad toggle"
   XF86TouchpadToggle
