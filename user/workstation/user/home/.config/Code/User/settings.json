{
    "redhat.telemetry.enabled": false,
    "breadcrumbs.enabled": true,
    "terminal.integrated.fontFamily": "hack,fontawesome",
    "editor.fontFamily": "hack,fontawesome",
    "editor.renderWhitespace": "all",
    "editor.scrollBeyondLastLine": false,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.selectionClipboard": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.rules": "javascript",
        "*.desktop": "ini"
    },
    "files.autoSave": "onFocusChange",
    "files.defaultLanguage": "markdown",
    "telemetry.telemetryLevel": "off",
    "typescript.referencesCodeLens.enabled": true,
    "workbench.editor.showTabs": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.tree.indent": 20,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.workspaceSymbols": "All",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "cmake.configureOnOpen": true,
    "editor.mouseWheelZoom": true,
    "workbench.colorTheme": "One Dark Pro Darker",
    "rust-client.channel": "stable",
    "rust-client.trace.server": "verbose",
    "rust.all_features": true,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.linkedProjects": [],
    "shellcheck.customArgs": [
        "-x"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "kotlin.compiler.jvm.target": "16",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "go.formatTool": "goimports",
    "workbench.startupEditor": "none",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/jvm/java-8-openjdk",
            "default": true
        },
        {
            "name": "JavaSE-19",
            "path": "/usr/lib/jvm/java-19-openjdk",
        },
    ],
    "gradle.debug": true,
    "java.eclipse.downloadSources": true,
    "kotlin.trace.server": "verbose",
    "vs-kubernetes": {
        "disable-linters": [
            "resource-limits"
        ],
        "vs-kubernetes.namespace": "",
        "vs-kubernetes.kubectl-path": "",
        "vs-kubernetes.helm-path": "",
        "vs-kubernetes.minikube-path": "",
        "vs-kubernetes.kubectlVersioning": "user-provided",
        "vs-kubernetes.outputFormat": "yaml",
        "vs-kubernetes.kubeconfig": "",
        "vs-kubernetes.knownKubeconfigs": [],
        "vs-kubernetes.autoCleanupOnDebugTerminate": false,
        "vs-kubernetes.nodejs-autodetect-remote-root": true,
        "vs-kubernetes.nodejs-remote-root": "",
        "vs-kubernetes.nodejs-debug-port": 9229,
        "vs-kubernetes.local-tunnel-debug-provider": "",
        "checkForMinikubeUpgrade": true,
        "imageBuildTool": "Docker"
    },
    "sqltools.format": {
        "reservedWordCase": "lower",
        "linesBetweenQueries": 2,
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "rust-analyzer.debug.engineSettings": {
    
    },
    "haxe.enableCodeLens": true,
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.external.linuxExec": "alacritty",
    "editor.guides.indentation": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-19-openjdk",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/home/nikarh/.vscode-oss/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
    "lldb.suppressUpdateNotifications": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 1,
    "workbench.editor.limit.perEditorGroup": true,
    "rust-analyzer.imports.granularity.enforce": true,
    "prettier.documentSelectors": [
        "**/*.{ts,js,css,scss,json,yml,yaml}"
    ],
}