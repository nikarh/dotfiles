# Increase volume
"~/.config/Xorg/control.sh volume inc"
   XF86AudioRaiseVolume

# Decrease volume
"~/.config/Xorg/control.sh volume dec"
   XF86AudioLowerVolume

# Decrease volume
"~/.config/Xorg/control.sh volume toggle"
   XF86AudioMute

"xbacklight -inc 8"
   XF86MonBrightnessUp

"xbacklight -dec 8"
   XF86MonBrightnessDown

"~/.config/Xorg/control.sh touchpad toggle"
   XF86TouchpadToggle
