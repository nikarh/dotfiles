{
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "boundary",
    "editor.renderIndentGuides": true,
    "editor.scrollBeyondLastLine": false,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.rules": "javascript",
        "*.desktop": "ini"
    },
    "files.autoSave": "onFocusChange",
    "files.defaultLanguage": "markdown",
    "telemetry.enableTelemetry": false,
    "typescript.referencesCodeLens.enabled": true,
    "workbench.editor.showTabs": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.tree.indent": 20,
    "C_Cpp.updateChannel": "Insiders",
    "editor.fontFamily": "Hack",
    "customizeUI.font.regular": "'Noto Sans'",
    "[cpp]": {},
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer",
        "editor.formatOnSave": true
    },
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.workspaceSymbols": "All",
    "cmake.configureOnOpen": true,
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "editor.mouseWheelZoom": true,
    "workbench.colorTheme": "One Dark Pro",
    "liveshare.allowGuestDebugControl": true,
    "liveshare.allowGuestTaskControl": true,
    "liveshare.featureSet": "insiders",
    "liveshare.languages.allowGuestCommandControl": true,
    "rust-client.channel": null,
    "rust-client.trace.server": "verbose",
    "rust.all_features": true,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.linkedProjects": [],
    "redhat.telemetry.enabled": false,
    "shellcheck.customArgs": [
        "-x"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "kotlin.compiler.jvm.target": "16",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "go.formatTool": "goimports",
    "workbench.startupEditor": "none",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/java-8-openjdk",
        },
        {
            "name": "JavaSE-16",
            "path": "/usr/lib/java-16-openjdk",
            "default": true
        },
    ],
    "gradle.debug": true,
    "gradle.javaDebug": {
        "tasks": [
            "run",
            "runBoot",
            "test",
            "intTest",
            "integration"
        ]
    },
    "java.eclipse.downloadSources": true
}