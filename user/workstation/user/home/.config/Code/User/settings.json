{
    "redhat.telemetry.enabled": false,
    "breadcrumbs.enabled": true,
    "terminal.integrated.fontFamily": "Hack Nerd Font Propo",
    "editor.fontFamily": "Hack Nerd Font Propo",
    "editor.renderWhitespace": "all",
    "editor.scrollBeyondLastLine": false,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.selectionClipboard": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.rules": "javascript",
        "*.desktop": "ini"
    },
    "files.autoSave": "onFocusChange",
    "files.defaultLanguage": "markdown",
    "telemetry.telemetryLevel": "off",
    "typescript.referencesCodeLens.enabled": true,
    "workbench.editor.showTabs": "single",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.tree.indent": 20,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.workspaceSymbols": "All",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "cmake.configureOnOpen": true,
    "editor.mouseWheelZoom": true,
    "workbench.colorTheme": "One Dark Pro Darker",
    "rust-analyzer.linkedProjects": [],
    "security.workspace.trust.untrustedFiles": "open",
    "kotlin.compiler.jvm.target": "16",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "go.formatTool": "goimports",
    "workbench.startupEditor": "none",
    "java.configuration.runtimes": [
        
        {
            "name": "JavaSE-17",
            "path": "/usr/lib/jvm/java-17-openjdk",
            "default": true
        },
    ],
    "gradle.debug": true,
    "java.eclipse.downloadSources": true,
    "kotlin.trace.server": "verbose",
    "sqltools.format": {
        "reservedWordCase": "lower",
        "linesBetweenQueries": 2,
    },
    "editor.suggestSelection": "first",
    "rust-analyzer.debug.engineSettings": {
    
    },
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.enablePersistentSessions": false,
    "editor.guides.indentation": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-21-openjdk",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/home/nikarh/.vscode-oss/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 1,
    "workbench.editor.limit.perEditorGroup": true,
    "rust-analyzer.imports.granularity.enforce": true,
