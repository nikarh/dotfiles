"~/.config/Xorg/volume-control.sh raise"
   XF86AudioRaiseVolume

"~/.config/Xorg/volume-control.sh lower"
   XF86AudioLowerVolume

"~/.config/Xorg/volume-control.sh mute"
   XF86AudioMute

"light -S 100"
   XF86MonBrightnessUp

"light -S 1"
   XF86MonBrightnessDown

"~/.config/Xorg/toggle-touchpad.sh"
   XF86TouchpadToggle

"gnome-screenshot -ac"
   Print
   
"gnome-screenshot -ic"
   Control + Print

"~/.config/Xorg/toggle-coolerboost.sh"
   Scroll_Lock

