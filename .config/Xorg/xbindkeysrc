# Increase volume
"~/.config/Xorg/control/xcontrol volume inc"
   XF86AudioRaiseVolume

# Decrease volume
"~/.config/Xorg/control/xcontrol volume dec"
   XF86AudioLowerVolume

# Decrease volume
"~/.config/Xorg/control/xcontrol volume toggle"
   XF86AudioMute

"light -S 100"
   XF86MonBrightnessUp

"light -S 1"
   XF86MonBrightnessDown

"~/.config/Xorg/control.sh touchpad toggle"
   XF86TouchpadToggle

"gnome-screenshot -ac"
   Print

"gnome-screenshot -ic"
   Control + Print

