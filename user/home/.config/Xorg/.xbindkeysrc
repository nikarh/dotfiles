"volume-control.sh raise"
   XF86AudioRaiseVolume

"volume-control.sh lower"
   XF86AudioLowerVolume

"volume-control.sh mute"
   XF86AudioMute

"light -S 100"
   XF86MonBrightnessUp

"light -S 1"
   XF86MonBrightnessDown

"toggle-touchpad.sh"
   XF86TouchpadToggle

"gnome-screenshot -ac"
   Print
   
"gnome-screenshot -ic"
   Control + Print

"toggle-coolerboost.sh"
   Scroll_Lock

